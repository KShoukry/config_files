# ADD LOCAL CONFIGURATION HERE


# DO NOT EDIT BELOW THIS LINE
[push]
  default = tracking
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
[format]
  pretty = %Cblue%h%Creset %Cgreen[%cr]%Creset (%an) %s
[alias]
  b = branch -a -v
  c = commit -m
  a = add
  co = checkout
  d = diff
  dc = diff --cached
  pl = pull
  ps = push
  plre = pull --rebase
  s = status
  out = log origin..HEAD
  tip = log HEAD^..HEAD
  lg = log -p
  r = remote -v
  me = log --date=local --pretty=short --author=pitr.vern@gmail.com --stat
  qpl = log --pretty=\"%Cblue%h%Creset %Cgreen[%cr]%Creset %s\" origin/production..origin/master
  g = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  ci = commit
  ca = commit --amend
  st = stash
[user]
  name = pitr
  email = pitr.vern@gmail.com
