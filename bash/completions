# enable programmable completion features (you don't need to enable
# this, if it's already enabled in /etc/bash.bashrc and /etc/profile
# sources /etc/bash.bashrc).
if [ -f /etc/bash_completion ]; then
    . /etc/bash_completion
fi

if [ -f /usr/local/etc/bash_completion.d/npm-completion.sh ]; then
  source /usr/local/etc/bash_completion.d/npm-completion.sh
fi

complete -C ~/.bash/rake_completion -o default rake
complete -C ~/.bash/project_completion -o default c
source ~/.bash/git_completion